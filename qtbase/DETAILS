           SPELL=qtbase
          SPELLX=$SPELL-opensource-src
         VERSION=5.1.0
     SOURCE_HASH=sha512:300f00045ba974d980859613f1409e3a9fb9a8405a1b6efaa03eae22fdd3c5e74e2fc2bf526e0ce3f0a80559a79a25ae64b3ab709fd0f06343fcdca7683ffe02
          SOURCE=$SPELLX-$VERSION.tar.xz
      SOURCE_URL=$QT_URL/qt/${VERSION%.?}/$VERSION/submodules/$SOURCE
